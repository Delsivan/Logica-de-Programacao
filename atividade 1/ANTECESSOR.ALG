Algoritmo "antecessor"
// Autor(a)    : Delsivan
// Data atual  : 08/09/2021
Var
   numero, antecessor: inteiro

Inicio
      escreva("Informe um número: ")
      leia(numero)
      escreval()
      
      antecessor <- numero-1
      escreva("O antecessor do número ", numero, " é o: ", antecessor)
      escreval()

Fimalgoritmo