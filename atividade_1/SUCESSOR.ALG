Algoritmo "sucessor"
// Autor(a)    : Delsivan
// Data atual  : 08/09/2021
Var
   numero, sucessor: inteiro

Inicio
      escreva("Informe um número: ")
      leia(numero)
      escreval()

      sucessor <- numero+1
      escreva("O sucessor do número ", numero, " é o: ", sucessor)
      escreval()

Fimalgoritmo