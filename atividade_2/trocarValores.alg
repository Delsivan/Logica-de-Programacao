algoritmo "trocarValores"
// Autor : Delsivan
// Data : 14/09/2021
var
   A, B, auxA, auxB: caracter

inicio
      escreva("Digite o valor de A: ")
      leia(A)
      escreva("Digite o valor de B: ")
      leia(B)

      
      auxA <- B
      auxB <- A
      
      A <- AuxA
      B <- AuxB
      
      escreval("O valor de A é: ", A)
      escreval("O valor de B é: ", B)
      
fimalgoritmo
